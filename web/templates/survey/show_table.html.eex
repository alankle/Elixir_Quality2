<!-- Table -->
        <table class="table">   
          <%= for subtheme <- @subthemes do %> 
          
            <tr class="form-group">
              <td>
                <%= subtheme.title %>
              </td>
              <td>
                <%= subtheme.note %>
              </td>              
            </tr>
          <% end %>
          <tr>
          <td><b>Moyenne: </b></td>
          <td><b><%= Float.to_string(@subthemes_average, decimals: 2)  %></b></td>
          </tr>
        </table>